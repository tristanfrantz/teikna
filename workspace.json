{
  "version": 1,
  "projects": {
    "teikna-e2e": {
      "root": "apps/teikna-e2e",
      "sourceRoot": "apps/teikna-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/teikna-e2e/cypress.json",
            "tsConfig": "apps/teikna-e2e/tsconfig.e2e.json",
            "devServerTarget": "fronty:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "fronty:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/teikna-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "api-interfaces": {
      "root": "libs/api-interfaces",
      "sourceRoot": "libs/api-interfaces/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/api-interfaces/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/api-interfaces/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "backy": {
      "root": "apps/backy",
      "sourceRoot": "apps/backy/src",
      "projectType": "application",
      "prefix": "api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/backy",
            "main": "apps/backy/src/main.ts",
            "tsConfig": "apps/backy/tsconfig.app.json",
            "assets": [
              "apps/backy/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/backy/src/environments/environment.ts",
                  "with": "apps/backy/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "backy:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/backy/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/backy/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "fronty": {
      "root": "apps/fronty",
      "sourceRoot": "apps/fronty/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/fronty",
            "index": "apps/fronty/src/index.html",
            "main": "apps/fronty/src/main.tsx",
            "polyfills": "apps/fronty/src/polyfills.ts",
            "tsConfig": "apps/fronty/tsconfig.app.json",
            "assets": [
              "apps/fronty/src/favicon.ico",
              "apps/fronty/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/fronty/src/environments/environment.ts",
                  "with": "apps/fronty/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "fronty:build",
            "proxyConfig": "apps/fronty/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "fronty:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/fronty/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/fronty/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "schematics": {
    "@nrwl/react": {
      "application": {
        "style": "styled-components",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "styled-components"
      },
      "library": {
        "style": "styled-components",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "fronty"
}
